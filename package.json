{
  "name": "Nest-js starter",
  "version": "0.0.1",
  "description": "",
  "author": "Muhammad Shaeel",
  "license": "",
  "scripts": {
    "build": "RMDIR /Q/S dist && tsc -p tsconfig.build.json && copy package.json dist && copy web.config dist && mkdir dist\\src\\config  && Xcopy src\\config  dist\\src\\config /E",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/websockets": "^7.6.13",
    "cache-manager": "^3.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cluster": "^0.7.7",
    "color-thief-node": "^1.0.4",
    "compression": "^1.7.4",
    "cryptr": "^4.0.2",
    "date-fns": "^1.30.1",
    "express-rate-limit": "^5.2.5",
    "fcm-node": "^1.5.2",
    "firebase-admin": "^9.5.0",
    "jimp": "^0.16.1",
    "moment": "^2.29.1",
    "mongodb-memory-server": "^6.9.6",
    "mongoose": "^5.6.4",
    "multer": "^1.4.2",
    "nest-status-monitor": "^0.1.4",
    "nestjs-fcm": "^0.1.0",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "request-ip": "^2.1.3",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.7",
    "unique-names-generator": "^4.4.0",
    "uuid": "^3.3.2",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.11",
    "@types/cache-manager": "^3.4.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/mongoose": "^5.5.8",
    "@types/node": "^10.12.18",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.7",
    "@types/validator": "^10.11.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
